<div class="container">
        
{{#each data}}
        <div class="card">
            <div class="row">
                <div class="card-title" style="font-weight: bold;">
                    {{this.title}}
                </div>
            </div>
            <div class="row">
                <div class="col m4">
                    <div class="image-div">
                        <img class="float-left" style="max-width:250px" src="{{this.imageUrl}}">
                    </div>
                </div>
                <div class="col m8">
                    <div class="card-content">
                        <div class="card-text pl-2">{{this.summary}} 
                    </div>
                </div> 
                <button>Save</button>  
            </div>      
        </div>
{{/each}}



</div>
