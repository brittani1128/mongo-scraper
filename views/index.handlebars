<div class="container">
            
{{#each data}}
        <div class="card">
            <div class="row">
                <div class="card-title" style="font-weight: bold;">
                    {{data.title}}
                </div>
            </div>
            <div class="row">
                <div class="col m3">
                    <div class="image-div">
                        <img class="float-left" src="{{data.imageUrl}}">
                    </div>
                </div>
                <div class="col m9">
                    <div class="card-content">
                        <div class="card-text pl-2">{{data.summary}}</a>
                    </div>
                </div> 
                <button>Save</button>  
            </div>      
        </div>
{{/each}}



</div>
